<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crystallake.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity
            android:name=".SecondActivity"
            android:exported="false" />
        <activity android:name=".transition.ChangeBoundsActivity"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".transition.share.NextShareElementTransitionActivity"/>
        <activity android:name=".transition.share.SampleShareElementTransitionActivity"/>
        <activity android:name=".transition.share.ShareElementTransitionMainActivity"
            />
        <activity android:name=".transition.browse.BrowseShareElementTransitionActivity"
            />
        <activity android:name=".transition.browse.ActivityBrowse"
            />
    </application>

</manifest>